{
  "name": "sorted-ranges",
  "version": "1.0.0",
  "author": {
    "name": "Tomer Aberbach",
    "email": "tomeraberbach@gmail.com",
    "url": "https://tomeraberba.ch"
  },
  "description": "A lightweight and performant sorted range map and set.",
  "keywords": [
    "disjoint",
    "map",
    "minimal",
    "range",
    "set",
    "sorted"
  ],
  "homepage": "https://github.com/TomerAberbach/sorted-ranges",
  "repository": "TomerAberbach/sorted-ranges",
  "bugs": {
    "url": "https://github.com/TomerAberbach/sorted-ranges/issues"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">= 18"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not op_mini all"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.min.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.min.cjs"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.min.cjs",
  "module": "./dist/index.min.js",
  "scripts": {
    "prepare": "tomer install",
    "format": "tomer format",
    "lint": "tomer lint",
    "typecheck": "tomer typecheck",
    "test": "tomer test",
    "build": "tomer build",
    "clean": "tomer clean dist",
    "prepublishOnly": "tomer build"
  },
  "prettier": "tomer/prettier",
  "devDependencies": {
    "@fast-check/vitest": "^0.1.3",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^8.57.0",
    "jsdom": "^25.0.0",
    "lfi": "^3.2.0",
    "prettier": "^3.3.3",
    "tomer": "^4.2.0",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5"
  }
}
